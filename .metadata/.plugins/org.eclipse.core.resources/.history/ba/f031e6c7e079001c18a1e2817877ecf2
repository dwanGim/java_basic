package interaction;

public class Seller {
	// 변수로 int money(매출), int mango(재고)를 가집니다.
	private int money;
	private int mango;
	
	// 
	public Seller(int mango) {
		this.money = 0;
		this.mango = mango;
	}
	
	// showSeller
	public void showSeller() {
		System.out.println("상인의 정보입니다.");
		System.out.println("현재 매출 : "+this.money+", 현재 재고 : "+this.mango);
	}
}
