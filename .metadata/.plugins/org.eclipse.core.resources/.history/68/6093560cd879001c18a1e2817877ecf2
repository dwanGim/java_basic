package accessmodifier;

public class Main01 {
	
	public static void main(String[] args) {
		// Magician을 하나 생성해주세요.
		Magician user1 = new Magician("해리포터");
		// getInfo()로 정보를 조회해주세요.
		user1.getInfo();
		// hunt()를 2번 호출해 사냥 2번
		user1.hunt();
		user1.hunt();
		user1.fireball();
		
		user1.getInfo();
	}
}
